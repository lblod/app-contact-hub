{
    "persist_indexes": true,
    "automatic_index_updates": true,
    "types": [
        {
            "type": "representative-body",
            "on_path": "representative-bodies",
            "rdf_type": "http://data.lblod.info/vocabularies/erediensten/RepresentatiefOrgaan",
            "properties": {
                "recognized-worship-type": {
                    "via" : "http://data.lblod.info/vocabularies/erediensten/typeEredienst",
                     "rdf_type" : "http://lblod.data.gift/vocabularies/organisatie/TypeEredienst",
                    "properties": {
                        "label": "http://www.w3.org/2004/02/skos/core#prefLabel"
                    }
                },
                "organization-status": {
                    "via" : "http://www.w3.org/ns/regorg#orgStatus",
                     "rdf_type" : "http://lblod.data.gift/vocabularies/organisatie/OrganisatieStatusCode",
                    "properties": {
                        "label": "http://www.w3.org/2004/02/skos/core#prefLabel"
                    }
                },
                "structured-identifier": {
                    "via" : "http://www.w3.org/ns/adms#identifier",
                     "rdf_type" : "http://www.w3.org/ns/adms#Identifier",
                    "properties": {
                        "local-id": ["https://data.vlaanderen.be/ns/generiek#gestructureerdeIdentificator","https://data.vlaanderen.be/ns/generiek#lokaleIdentificator"]
                    }
                },
                "address": {
                    "via" : "http://www.w3.org/ns/org#hasPrimarySite",
                     "rdf_type" : "http://www.w3.org/ns/org#Site",
                    "properties": {
                        "number": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","https://data.vlaanderen.be/ns/adres#Adresvoorstelling.huisnummer"],
                        "box-number": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","https://data.vlaanderen.be/ns/adres#Adresvoorstelling.busnummer"],
                        "street": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","http://www.w3.org/ns/locn#thoroughfare"],
                        "postcode": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","http://www.w3.org/ns/locn#postCode"],
                        "municipality": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","http://www.w3.org/ns/locn#gemeentenaam"],
                        "province": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","http://www.w3.org/ns/locn#adminUnitL2"],
                        "country": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","https://data.vlaanderen.be/ns/adres#land"],
                        "full-address": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","http://www.w3.org/ns/locn#fullAddress"],
                        "address-register-uri": ["https://data.vlaanderen.be/ns/organisatie#bestaatUit","https://data.vlaanderen.be/ns/adres#verwijstNaar"]
                    }
                },
                "contacts": {
                    "via" : "http://www.w3.org/ns/org#hasPrimarySite",
                     "rdf_type" : "http://www.w3.org/ns/org#Site",
                    "properties": {
                        "email": ["http://www.w3.org/ns/org#siteAddress","http://schema.org/email"],
                        "telephone": ["http://www.w3.org/ns/org#siteAddress","http://schema.org/telephone"],
                        "website": ["http://www.w3.org/ns/org#siteAddress","http://xmlns.com/foaf/0.1/page"],
                        "contact-address": ["http://www.w3.org/ns/org#siteAddress","http://www.w3.org/ns/locn#address"]
                    }
                },
                "is-associated-with":{
                    "via" : "http://www.w3.org/ns/org#linkedTo",
                     "rdf_type" : "http://www.w3.org/ns/org#Organization",
                    "properties": {
                        "classification": ["http://www.w3.org/ns/org#classification","http://www.w3.org/2004/02/skos/core#prefLabel"]
                    }

                }
            },
            "mappings": {
              "properties": {

                "address.number": { "type" : "keyword"},
                "address.box": { "type" : "text" },
                "address.street": { "type" : "text" },
                "address.postcode": { "type" : "text" },
                "address.municipality": { "type" : "text" },
                "address.province": { "type" : "text" },
                "address.country": { "type" : "text" },
                "address.full-address": { "type" : "text" },
                "address.address-register-uri": { "type" : "text" },
                "recognized-worship-type.label": { "type" : "text" },
                "organization-status.label": { "type" : "text" },
                "structured-identifier.local-id": { "type" : "text" },
                "contacts.telephone": { "type" : "text" },
                "contacts.website": { "type" : "text" },
                "contacts.contact-address": { "type" : "text" },
                "is-associated-with.classification": { "type" : "text" }
              }
            }
        },
        {
            "type": "central-worship-service",
            "on_path": "central-worship-services",
            "rdf_type": "http://data.lblod.info/vocabularies/erediensten/CentraalBestuurVanDeEredienst",
            "properties": {
                "contacts": {
                    "via" : "http://www.w3.org/ns/org#hasPrimarySite",
                     "rdf_type" : "http://www.w3.org/ns/org#Site",
                    "properties": {
                        "email": ["http://www.w3.org/ns/org#siteAddress","http://schema.org/email"],
                        "telephone": ["http://www.w3.org/ns/org#siteAddress","http://schema.org/telephone"],
                        "website": ["http://www.w3.org/ns/org#siteAddress","http://xmlns.com/foaf/0.1/page"],
                        "contact-address": ["http://www.w3.org/ns/org#siteAddress","http://www.w3.org/ns/locn#address"]
                    }
                },
                "is-associated-with":{
                    "via" : "http://www.w3.org/ns/org#linkedTo",
                     "rdf_type" : "http://www.w3.org/ns/org#Organization",
                    "properties": {
                        "classification": ["http://www.w3.org/ns/org#classification","http://www.w3.org/2004/02/skos/core#prefLabel"]
                    }

                },
                "sub-organizations":{
                    "via" : "http://www.w3.org/ns/org#hasSubOrganization",
                     "rdf_type" : "http://www.w3.org/ns/org#Organization",
                    "properties": {
                        "classification": ["^http://www.w3.org/ns/org#classification","http://www.w3.org/2004/02/skos/core#prefLabel"]
                    }

                },
                "is-sub-organization-of":{
                    "via" : "http://www.w3.org/ns/org#hasSubOrganization",
                     "rdf_type" : "http://www.w3.org/ns/org#Organization",
                    "properties": {
                        "classification": ["http://www.w3.org/ns/org#classification","http://www.w3.org/2004/02/skos/core#prefLabel"]
                    }

                }
            },
            "mappings": {
              "properties": {
                "contacts.telephone": { "type" : "text" },
                "contacts.website": { "type" : "text" },
                "contacts.contact-address": { "type" : "text" },
                "sub-organizations.classification": { "type" : "text" },
                "is-sub-organization-of.classification": { "type" : "text" }
              }
            }
        }
    ]
}