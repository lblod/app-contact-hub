{
  "version": "0.1",
  "prefixes": {
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "org": "http://www.w3.org/ns/org#",
    "locn": "http://www.w3.org/ns/locn#",
    "organisatie": "https://data.vlaanderen.be/ns/organisatie#",
    "dc_terms": "http://purl.org/dc/terms/",
    "schema": "http://schema.org/",
    "regorg": "http://www.w3.org/ns/regorg#",
    "person": "http://www.w3.org/ns/person#",
    "persoon": "https://data.vlaanderen.be/ns/persoon#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "vcard": "http://www.w3.org/2006/vcard/ns#",
    "adres": "https://data.vlaanderen.be/ns/adres#",
    "generiek": "https://data.vlaanderen.be/ns/generiek#",
    "euro": "http://data.europa.eu/m8g/",
    "adms": "http://www.w3.org/ns/adms#",
    "lblodlg": "https://data.lblod.info/vocabularies/leidinggevenden/",
    "besluit": "http://data.vlaanderen.be/ns/besluit#",
    "mandaat": "http://data.vlaanderen.be/ns/mandaat#",
    "ere": "http://data.lblod.info/vocabularies/erediensten/",
    "ch": "http://data.lblod.info/vocabularies/contacthub/"
  },
  "resources": {
    "people": {
      "name": "person",
      "class": "person:Person",
      "attributes": {
        "given-name": {
          "type": "string",
          "predicate": "foaf:givenName"
        },
        "family-name": {
          "type": "string",
          "predicate": "foaf:familyName"
        },
        "first-name-used": {
          "type": "string",
          "predicate": "persoon:gebruikteVoornaam"
        }
      },
      "relationships": {
        "mandatories": {
          "predicate": "mandaat:isAangesteldAls",
          "target": "mandatory",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/persoon/"
    },
    "mandatories": {
      "name": "mandatory",
      "class": "mandaat:Mandataris",
      "attributes": {
        "start-date": {
          "type": "dateTime",
          "predicate": "mandaat:start"
        },
        "end-date": {
          "type": "dateTime",
          "predicate": "mandaat:einde"
        }
      },
      "relationships": {
        "status": {
          "predicate": "mandaat:status",
          "target": "mandatoryStatusCode",
          "cardinality": "one"
        },
        "governingAlias": {
          "predicate": "mandaat:isBestuurlijkeAliasVan",
          "target": "person",
          "cardinality": "one"
        },
        "contactPoint": {
          "predicate": "schema:contactPoint",
          "target": "contactPoint",
          "cardinality": "many"
        },
        "mandate": {
          "predicate": "org:holds",
          "target": "mandate",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/mandataris/"
    },
    "mandatory-status-codes": {
      "name": "mandatoryStatusCode",
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "new-resource-base": "http://data.vlaanderen.be/id/concept/MandatarisStatusCode/"
    },
    "contact-points": {
      "name": "contactPoint",
      "class": "schema:ContactPoint",
      "attributes": {
        "email": {
          "type": "string",
          "predicate": "schema:email"
        },
        "telephone": {
          "type": "string",
          "predicate": "schema:telephone"
        },
        "fax": {
          "type": "string",
          "predicate": "schema:faxNumber"
        },
        "website": {
          "type": "uri",
          "predicate": "foaf:page"
        }
      }, 
      "relationships": {
        "contact-address": {
          "predicate": "locn:address",
          "target": "address",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/contactpunt/"
    },
    "mandates": {
      "name": "mandate",
      "class": "Mandaat",
      "relationships": {
        "role": {
          "predicate": "org:role",
          "target": "roleConcept",
          "cardinality": "one"
        },
        "governingBodies": {
          "predicate": "org:hasPost",
          "target": "governingBody",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/mandaat/"
    },
    "governing-bodies": {
      "name": "governingBody",
      "class": "besluit:Bestuursorgaan",
      "attributes": {
        "start-date": {
          "type": "dateTime",
          "predicate": "mandaat:bindingStart"
        },
        "end-date": {
          "type": "dateTime",
          "predicate": "mandaat:bindingEinde"
        }
      },
      "relationships": {
        "administrativeUnit": {
          "predicate": "besluit:bestuurt",
          "target": "administrativUnit",
          "cardinality": "one"
        },
        "classification": {
          "predicate": "org:classification",
          "target": "governingBodyClassificationCode",
          "cardinality": "one"
        },
        "isTimeSpecializationOf": {
          "predicate": "generiek:isTijdspecialisatieVan",
          "target": "governingBody",
          "cardinality": "one"
        },
        "hasTimeSpecializations": {
          "predicate": "generiek:isTijdspecialisatieVan",
          "target": "goveringBody",
          "cardinality": "many",
          "inverse": true
        },
        "contains": {
          "predicate": "org:hasPost",
          "target": "mandate",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/bestuursorgaan/"
    },
    "governing-body-classification-codes": {
      "name": "governingBodyClassificationCode",
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "new-resource-base": "http://data.vlaanderen.be/id/concept/BestuursorgaanClassificatieCode/"
    },
    "administrative-units": {
      "name": "administrativeUnit",
      "class": "besluit:Bestuurseenheid",
      "attributes": {
        "name": {
          "type": "string",
          "predicate": "skos:prefLabel"
        },
        "alternative-name": {
          "type":  "string",
          "predicate": "skos:altLabel"
        }
      },
      "relationships": {
        "classification": {
          "predicate": "org:classification",
          "target": "administrativeUnitClassificationCode",
          "cardinality": "one"
        },
        "primary-site": {
          "predicate": "org:hasPrimarySite",
          "target": "site",
          "cardinality": "one"
        },
        "identifiers": {
          "predicate": "adms:idenfier",
          "target": "identifier",
          "cardinality": "many"
        },
        "governing-bodies": {
          "predicate": "besluit:bestuurt",
          "target": "governingBody",
          "cardinality": "many",
          "inverser": true
        },
        "contact-points": {
          "predicate": "schema:contactPoint",
          "target": "contactPoint",
          "cardinality": "many"
        },
        "sites": {
          "predicate": "org:hasSite",
          "target": "site",
          "cardinality": "many"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/bestuurseenheid/"
    },
    "administrative-unit-classification-codes": {
      "name": "administrativeUnitClassificationCode",
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "new-resource-base": "http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/"
    },
    "identifiers": {
      "name": "identifier",
      "class": "adms:Identifier",
      "attributes": {
        "id-name": {
          "type": "string",
          "predicate": "skos:notation"
        }
      },
      "relationships": {
        "structured-identifier": {
          "predicate": "generiek:gestructureerdeIdentificator",
          "target": "structuredIdentifier",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/identificator/"
    },
    "structured-idenfiers": {
      "name": "structuredIdentifier",
      "class": "generiek:GestructureerdeIdentificator",
      "attributes": {
        "local-id": {
          "type": "string",
          "predicate": "generiek:lokaleIdentificator"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/gestructureerdeIdentificator/"
    },
    "addresses": {
      "name": "address",
      "class": "locn:Address",
      "attributes": {
        "number": {
          "type": "string",
          "predicate": "adres:Adresvoorstelling.huisnummer"
        },
        "box-number": {
          "type": "string",
          "predicate": "adres:Adresvoorstelling.busnummer"
        },
        "street": {
          "type": "string",
          "predicate": "locn:thoroughfare"
        },
        "postcode": {
          "type": "string",
          "predicate": "locn:postCode"
        },
        "municipality": {
          "type": "string",
          "predicate": "adres:gemeentenaam"
        },
        "province": {
          "type": "string",
          "predicate": "locn:adminUnitL2"
        },
        "country": {
          "type": "string",
          "predicate": "adres:land"
        },
        "full-address": {
          "type": "string",
          "predicate": "locn:fullAddress"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/adres/"
    },
    "sites": {
      "name": "site",
      "class": "org:Site",
      "relationships": {
        "contact": {
          "predicate": "org:siteAddress",
          "target": "contactPoint",
          "cardinality": "many"
        },
        "address": {
          "predicate": "organisatie:bestaatUit",
          "target": "address",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/vestiging/"
    },
    "worship-services": {
      "name": "worshipService",
      "class": "ere:BestuurVanDeEredienst",
      "super": ["administrativeUnit"],
      "relationships": {
        "type": {
          "predicate": "ere:typeEredienst",
          "target": "eredienstType",
          "cardinality": "one"
        }
      },
      "new-resource-base": "http://data.lblod.info/id/bestuurVanDeEredienst/"
    }
  }
}