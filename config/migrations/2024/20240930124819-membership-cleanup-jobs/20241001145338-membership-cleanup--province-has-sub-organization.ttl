<http://data.lblod.info/id/cleanup-job/434b8b52-b695-4358-a927-ddaceb22e7e3> a cleanup:Job ;
    mu:uuid "434b8b52-b695-4358-a927-ddaceb22e7e3" ;
    dcterms:title "Insert missing `org:hasSubOrganization` triples for Provinces." ;
    cleanup:randomQuery """
      PREFIX org: <http://www.w3.org/ns/org#>

      INSERT {
        GRAPH <http://mu.semte.ch/graphs/administrative-unit> {
          ?organization org:hasSubOrganization ?member .
        }
      } WHERE {
        GRAPH <http://mu.semte.ch/graphs/administrative-unit> {
          ?membership a org:Membership ;
            org:organization ?organization ;
            org:member ?member ;
            org:role <http://data.lblod.info/id/rollen/4ec7d5c39bdc4e84b4174379b9e22ad8> .

          FILTER NOT EXISTS { ?organization org:hasSubOrganization ?member . }
        }

        ?organization org:classification <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/5ab0e9b8a3b2ca7c5e000000> .

        ?member org:classification ?memberClass .
        FILTER ( ?memberClass
                 IN (
                    <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/36a82ba0-7ff1-4697-a9dd-2e94df73b721> ,
                    <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/5ab0e9b8a3b2ca7c5e000001> ,
                    <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/5ab0e9b8a3b2ca7c5e000002> ,
                    <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/2ad46df5-5c79-4d67-84d5-604c1377231e> ,
                    <http://data.vlaanderen.be/id/concept/BestuurseenheidClassificatieCode/088784b6-e188-48bf-b94f-94665f9e1f53>
                    )
               )
    }
    """ .
