DELETE {
  GRAPH ?g {
    ?address
      locn:fullAddress
        ?full_address.
  }
}

INSERT {
  GRAPH ?g {
    ?address
      locn:fullAddress
        ?fixed_full_address.
  }
}

WHERE {
  GRAPH ?g {
    ?address
      locn:thoroughfare
        ?street;
      adres:Adresvoorstelling.huisnummer
        ?streetNumber;
      adres:Adresvoorstelling.busnummer
        ?busNumber;
      adres:gemeentenaam
        ?gemeente;
      locn:postCode
        ?postalCode;
      locn:fullAddress
        ?full_address.
    FILTER (strlen(?busNumber) > 0 )
    FILTER (CONTAINS(?full_address, CONCAT(?streetNumber, ?busNumber)))
    FILTER (STRSTARTS(?full_address, CONCAT(?street, ' ',?streetNumber, ?busNumber, ', ', ?postalCode, ' ', ?gemeente)))
    BIND (CONCAT(?street, ' ',?streetNumber, ' ' , ?busNumber, ', ', ?postalCode, ' ', ?gemeente, ", BelgiÃ«") as ?fixed_full_address)
  }
}